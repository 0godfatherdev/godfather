{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "dev": {
            "persistent": true,
            "cache": false,
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": ["^build"]
        }
    }
}
